{
  "name": "server",
  "version": "0.0.1",
  "description": "koa server boilerplate for koa-react-boilerplate project.",
  "main": "main.ts",
  "author": "Ben Stoddard",
  "license": "MIT",
  "private": false,
  "scripts": {
    "lint": "tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet",
    "fix": "yarn lint --fix",
    "build": "tsc",
    "local": "ts-node-dev --respawn -- main.ts",
    "start": "ts-node -r dotenv/config main.ts"
  },
  "dependencies": {
    "@koa/cors": "^3.1.0",
    "@types/gulp": "^4.0.8",
    "@types/koa": "^2.11.6",
    "@types/koa-bodyparser": "^4.3.0",
    "@types/koa-cors": "^0.0.0",
    "@types/koa-logger": "^3.1.1",
    "@types/koa-mount": "^4.0.0",
    "@types/koa-router": "^7.4.1",
    "@types/koa-static": "^4.0.1",
    "@types/koa__cors": "^3.0.2",
    "dotenv": "^8.2.0",
    "gulp-typescript": "^6.0.0-alpha.1",
    "koa": "^2.13.1",
    "koa-bodyparser": "^4.3.0",
    "koa-cors": "^0.0.16",
    "koa-logger": "^3.2.1",
    "koa-mount": "^4.0.0",
    "koa-router": "^10.0.0",
    "koa-static": "^5.0.0",
    "ts-node": "^9.1.1"
  }
}
